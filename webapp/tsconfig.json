{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "types": [
      "vite/client",
      "vitest/globals",
      "vite-plugin-svgr/client"
    ],
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "module": "ES2020",
    "moduleResolution": "Node",
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}
