{
  "name": "@serieslist/webapp",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "tsx ./src/server/server.ts",
    "start:e2e": "dotenv -e ../../.env.e2e pnpm start",
    "build": "rimraf dist && vite build && pnpm build:server && resolve-tspaths --out dist/prodServer",
    "build:server": "tsc -p tsconfig.server.json",
    "start:prod": "cross-env NODE_ENV=production node dist/prodServer/server/server.js",
    "lint": "eslint src && madge --circular --extensions ts,tsx ./src",
    "tsc": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "dotenv -e ../../.env.e2e pnpm exec playwright test",
    "codegen": "graphql-codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@fastify/compress": "^6.4.0",
    "@fastify/cookie": "^8.3.0",
    "@fastify/middie": "^8.3.0",
    "@fastify/static": "^6.10.2",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@hookform/resolvers": "^3.1.0",
    "@radix-ui/react-accessible-icon": "^1.0.3",
    "@radix-ui/react-alert-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.3",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.4",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@sentry/react": "^7.56.0",
    "@serieslist/prettier-config": "workspace:*",
    "@vanilla-extract/css": "^1.10.0",
    "@vitejs/plugin-react": "^4.1.0",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.1.0",
    "dotenv-expand": "^10.0.0",
    "fastify": "^4.18.0",
    "fishery": "^2.2.2",
    "graphql": "^16.8.1",
    "isomorphic-unfetch": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-highlight-words": "^0.20.0",
    "react-hook-form": "^7.43.9",
    "react-transition-group": "^4.4.5",
    "vike": "^0.4.142",
    "vite-plugin-svgr": "^4.1.0",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/client-preset": "4.0.1",
    "@playwright/test": "^1.35.1",
    "@sentry/vite-plugin": "^2.8.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/react-highlight-words": "^0.16.4",
    "@types/react-transition-group": "^4.4.6",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vanilla-extract/vite-plugin": "^3.9.0",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "jsdom": "^22.1.0",
    "madge": "^6.0.0",
    "mock-apollo-client": "^1.2.1",
    "nanoid": "^4.0.2",
    "prettier": "2.8.4",
    "resolve-tspaths": "^0.8.13",
    "rimraf": "^4.4.0",
    "tsx": "^3.14.0",
    "typescript": "^4.9.4",
    "vite": "^4.4.11",
    "vite-plugin-pwa": "^0.16.5",
    "vitest": "^0.34.6"
  }
}
